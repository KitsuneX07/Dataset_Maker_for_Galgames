[
    {
        "File": "良篇/翠儿/翠儿-01",
        "Text": "唔、唔……",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-02",
        "Text": "哇，大火！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-03",
        "Text": "呀！！！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-04",
        "Text": "蛇！有蛇！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-05",
        "Text": "不要！舌头爷！……兴爷不要剁！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-06",
        "Text": "呜呜……谢谢良爷……呜呜呜呜……",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-07",
        "Text": "姐姐！谢谢昨天把你滴粮分了额一块。",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-08",
        "Text": "嗯、穗姐姐！你怎么说话了！你不能在良爷面前说话滴！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-09",
        "Text": "呀！这水好烫！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-010",
        "Text": "额也记得！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-011",
        "Text": "哎呀，姐，想拉臭臭！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-012",
        "Text": "好——额能憋着！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-013",
        "Text": "姐，额想洗头！头痒了好几天了，怕有虱子！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-014",
        "Text": "要把头放到盆里吗？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-015",
        "Text": "姐！不要加了，水进耳朵里咧！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-016",
        "Text": "姐，额又想拉臭臭咧！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-017",
        "Text": "肉馒头……",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-018",
        "Text": "那姐姐也尝尝肉馒头，好吃！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-019",
        "Text": "姐姐吃，尝尝！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-020",
        "Text": "谢谢……良爷！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-021",
        "Text": "良爷？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-022",
        "Text": "之前……三爷说了……去洛阳才能吃饱饭，那里有吃不完的馒头。",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-023",
        "Text": "嗯！翠儿信良爷！良爷是好人！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-024",
        "Text": "鹰籽细是啥？好吃吗？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-025",
        "Text": "想吃鹰籽细！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-026",
        "Text": "嗯！良爷像是昨天的红袍子一样！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-027",
        "Text": "演好了有肉吃吗？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-028",
        "Text": "有肉！太好了！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-029",
        "Text": "姐姐，鬼是啥？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-030",
        "Text": "穗姐姐，鬼是啥？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-031",
        "Text": "呜……",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-032",
        "Text": "喔！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-033",
        "Text": "趁热吃啊！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-034",
        "Text": "不！我就想用嘛！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-035",
        "Text": "姐姐小气！哼！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-036",
        "Text": "嘿嘿，穗姐姐真好！咱先用刀切啥？大蒜还是白菜？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-037",
        "Text": "唔！良爷！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-038",
        "Text": "良爷，你看！我会切菜！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-039",
        "Text": "我也要试试！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-040",
        "Text": "哼！姐姐真讨厌，又不让我试！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-041",
        "Text": "好，良爷一定要多吃点！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-042",
        "Text": "来——良爷吃。",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-043",
        "Text": "穗姐姐做得白菜是我切的！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-044",
        "Text": "姐姐不用给我夹，我会自己夹！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-045",
        "Text": "没生气！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-046",
        "Text": "良爷，什么事？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-047",
        "Text": "小二挺好的！带我们去了厨房。",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-048",
        "Text": "嗯……感觉是好姐姐！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-049",
        "Text": "我想回家……想和爹娘弟弟在一起。",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-050",
        "Text": "不要！我要和良爷一起！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-051",
        "Text": "嗯！我也愿意留，不想和姐姐分开。",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-052",
        "Text": "良爷、还有穗姐姐、华姐姐以后一定要来看我们！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-053",
        "Text": "肉馒头！肉馒头！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-054",
        "Text": "礼物？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-055",
        "Text": "我想看！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-056",
        "Text": "哇——",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-057",
        "Text": "陶响球！这要怎么玩？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-058",
        "Text": "我听听……哇！是沙子的声音！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-059",
        "Text": "谢谢良爷！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-060",
        "Text": "良爷！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-061",
        "Text": "你要走了吗？",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-062",
        "Text": "我不想你走！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-063",
        "Text": "可是……我还是不想良爷走……不想穗姐姐走！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-064",
        "Text": "不要！不要！",
        "Name": "翠儿"
    },
    {
        "File": "良篇/翠儿/翠儿-065",
        "Text": "之后……良爷和穗姐姐真的会回来吗？",
        "Name": "翠儿"
    }
]