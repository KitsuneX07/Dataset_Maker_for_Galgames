[
    {
        "File": "siria006_001",
        "Text": "おーっす",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria007_001",
        "Text": "お前らまさか……",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria010_001",
        "Text": "おはよう。って、どうした？なんか眠そうだな",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria010_002",
        "Text": "そいつはご愁傷様",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_001",
        "Text": "はぁっ！？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_002",
        "Text": "はぁ？おい、マジかよ",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_003",
        "Text": "おい、マジかよ",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_004",
        "Text": "高嶺君さぁ。",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_005",
        "Text": "でもデートするんだろ？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_006",
        "Text": "前に見たあの写真、お前か！？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_007",
        "Text": "運動場？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_008",
        "Text": "ああ。パンケーキとかが有名な店なんだろ？話くらいは聞いたことあるぞ。行ったことはないけど",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_010",
        "Text": "ふーむ……バイトか。そういや、さっきも店がどうこう言ってたな",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_011",
        "Text": "残念だ。先にそのバイト先の存在を知ってたら……",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria203_012",
        "Text": "いやまて。今ここで、そのバイトの席を空白にしてしまえば",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_001",
        "Text": "川？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_002",
        "Text": "前は、ただのバイト仲間って話だったろ？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_003",
        "Text": "犬？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_004",
        "Text": "有罪。死刑",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_005",
        "Text": "高嶺、自首しよう。脅迫は犯罪なんだぞ",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_006",
        "Text": "付き合うぜ、汐山！",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_007",
        "Text": "有罪",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_008",
        "Text": "ただの食事だって",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_009",
        "Text": "凄い話題になってるのに",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_010",
        "Text": "いいだろ、妬むくらい！",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_011",
        "Text": "でも俺たちは冗談半分だろ",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_012",
        "Text": "相手の言い方が不愉快だったのは、周りにいる人間も感じてたらしいから",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_013",
        "Text": "言わなきゃよかった",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_014",
        "Text": "そうだ。その幸せを分けてくれてもいいくらいだ！",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_015",
        "Text": "うっぜー",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_016",
        "Text": "で、どこの店だっけ？",
        "Name": "知り合いＡ"
    },
    {
        "File": "siria209_017",
        "Text": "あー、あそこか",
        "Name": "知り合いＡ"
    }
]