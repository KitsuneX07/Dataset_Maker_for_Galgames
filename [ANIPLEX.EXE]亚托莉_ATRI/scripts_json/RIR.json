[
    {
        "File": "RIR_b111_001",
        "Text": "あっ……ヒットマンだ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_004",
        "Text": "や……殺られるっ！！？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_005",
        "Text": "あたしが行ってくる",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_006",
        "Text": "任せとけ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_007",
        "Text": "来てやったぞヒットマン！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_008",
        "Text": "ザコは引っ込んでろ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_009",
        "Text": "ヒットマン、あたしと勝負しろ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_012",
        "Text": "そうだ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_013",
        "Text": "え、えっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_017",
        "Text": "くっそーー、みんなごめんなっ！！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_019",
        "Text": "みなもちゃんに何する気だ……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_021",
        "Text": "もしかして……告白しに来たのか？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_022",
        "Text": "みんな逃げてーー！！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_023",
        "Text": "逃げて、ヒットマンが来た！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_025",
        "Text": "ヒットマン",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_026",
        "Text": "近づいちゃダメだ、殺られる",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_028",
        "Text": "ここは通さないぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_029",
        "Text": "おい子分、取り押さえろ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b111_030",
        "Text": "うーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b112_001",
        "Text": "ヒットマン……鬼だ……鬼ヒットマンだ……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b112_002",
        "Text": "おー、また明日なー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_001",
        "Text": "あっ、ヒットマンだ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_002",
        "Text": "やるか、子分！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_003",
        "Text": "子分のクセにナマイキだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_005",
        "Text": "みなもちゃんはあたしが守る！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_006",
        "Text": "ハイ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_007",
        "Text": "つるたさんに先生になってもらう！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_010",
        "Text": "キョーメンインコ……なにそれ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_011",
        "Text": "遊びにいこー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_012",
        "Text": "ま、待って。。",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_013",
        "Text": "みなもちゃん、セーフ？セーフだよね？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_014",
        "Text": "セーフだった……危なかった",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_015",
        "Text": "トクベツこーし？誰だッ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_016",
        "Text": "おまえかー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_017",
        "Text": "わかった、爆弾を作るんだな！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_019",
        "Text": "あっち見にいこう！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_020",
        "Text": "そんなのみなもちゃんだけだよー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_021",
        "Text": "ふふふ、これだー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_022",
        "Text": "一番の大物だぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_023",
        "Text": "ええっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_025",
        "Text": "ほんと？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_027",
        "Text": "図工の時間だな！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_028",
        "Text": "す、すげー……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_029",
        "Text": "できたー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_030",
        "Text": "できたぞ、ヒットマン先生！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_031",
        "Text": "あっ、光った！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b113_035",
        "Text": "そっかー……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_002",
        "Text": "わっ、なんだ！？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_003",
        "Text": "お、オバケ？やだやだっ、食べないでー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_005",
        "Text": "ふぇ？ひ……ヒットマン！！？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_007",
        "Text": "ここがあたしの家だもん",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_008",
        "Text": "うん",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_009",
        "Text": "いない",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_012",
        "Text": "図書室。いっぱい本がある",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_015",
        "Text": "ふむふむ……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_018",
        "Text": "同じ方向に流れてる？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_019",
        "Text": "でも、高いところに水を運べばいいんでしょう？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_020",
        "Text": "エネルギー保存の法則かー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_021",
        "Text": "ううん、本に書いてあった",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_023",
        "Text": "ん？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_026",
        "Text": "うん、よく言われるー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_027",
        "Text": "なぁ、ヒットマン先生",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_028",
        "Text": "勉強はしても無駄なのか……？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_031",
        "Text": "もっと色んな事が知りたいって思った！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_035",
        "Text": "……オシッコ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_037",
        "Text": "そうなのか？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_038",
        "Text": "今ので止まっちゃった……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_039",
        "Text": "ふぅ、スッキリ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_040",
        "Text": "うん。シャワーもある",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_041",
        "Text": "夜は真っ暗で使えないけど",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_042",
        "Text": "あぅ……お腹空いたー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_045",
        "Text": "子分も食うか？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_046",
        "Text": "よかったな、子分",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_048",
        "Text": "なるよ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_051",
        "Text": "……ほんとは暗いのは嫌いだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_053",
        "Text": "今日作ったやつ、なんかあの豆球みたいなんだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_055",
        "Text": "また明日なーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_059",
        "Text": "わーー！\\\\n船に住んでんだ、かっけーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_060",
        "Text": "なつき先生のおうち、見てみたかったんだー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_061",
        "Text": "ラジオだ、いいなー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b114_062",
        "Text": "ほんと？やったー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_001",
        "Text": "な、すごいだろー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_002",
        "Text": "遠足楽しいなーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_004",
        "Text": "うわーー、みなもちゃんすごいスタイルいい！！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_009",
        "Text": "ええっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_010",
        "Text": "リュウちゃん、おっかないって言ってたぞ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_011",
        "Text": "カニー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_012",
        "Text": "カニカニ。。。",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_013",
        "Text": "いただきまーーす",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b121_014",
        "Text": "あたし、１年生の時、桃太郎やったよ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_001",
        "Text": "はいはい！原子力発電！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_002",
        "Text": "原子力も？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_003",
        "Text": "ハイ！渦ができるぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_005",
        "Text": "削った発泡スチロール、雪みたいだー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_006",
        "Text": "きゃははははは",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_008",
        "Text": "いただきまーーす",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_009",
        "Text": "わ、わかった",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_011",
        "Text": "みなもちゃんドンマイ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_012",
        "Text": "なつき先生、どんな子供だったんだー？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_013",
        "Text": "今と同じだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b122_015",
        "Text": "なつき先生、赤くなったぞ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_001",
        "Text": "んしょ……んしょ……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_002",
        "Text": "うん、先生に教えてもらったから",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_003",
        "Text": "あっ、しまった！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_004",
        "Text": "……なんでもないよ。ヒミツだから",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_006",
        "Text": "ぎくっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_007",
        "Text": "な、なんでもないよ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_009",
        "Text": "ごめんなさい……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_010",
        "Text": "……あたしが拾ってきたんだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_012",
        "Text": "……ん？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_014",
        "Text": "ハナちゃん、ここにいちゃダメ……？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_015",
        "Text": "ハナちゃん……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_016",
        "Text": "もう電気使える？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_020",
        "Text": "……？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_021",
        "Text": "ついたーー！！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_022",
        "Text": "夜なのに、本が読めるぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_023",
        "Text": "あははは、やったなー子分ー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_024",
        "Text": "夜に本読むのも不良？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_025",
        "Text": "おやすみなさい、なつき先生",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_026",
        "Text": "おはようなつき先生！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_027",
        "Text": "ハナちゃんだー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_028",
        "Text": "ほんとかっ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b123_029",
        "Text": "ハナちゃん先生、また一緒に住んでいいの！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b124_002",
        "Text": "お小遣い！絶対やるー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b124_003",
        "Text": "作戦会議だー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b124_004",
        "Text": "おはようございまーす",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_001",
        "Text": "お仕事だーー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_003",
        "Text": "はい、なつき先生",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_005",
        "Text": "凜々花ねー、なつき先生好きー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_006",
        "Text": "待って待って、まだ掃除してるんだっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_008",
        "Text": "図書室の本で読んだから知ってるんだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_010",
        "Text": "ウブかー。可愛いなぁ、なつき先生",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_013",
        "Text": "いいなー……ハンバーグ美味しそう",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_014",
        "Text": "わぁ。、ありがとうみなもちゃん！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b201_016",
        "Text": "お仕事したらもらったー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_001",
        "Text": "みなもちゃん……圧がすごいよ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_002",
        "Text": "アトリん、なつき先生と喧嘩したのか？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_005",
        "Text": "どいたどいたーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_006",
        "Text": "音楽の授業ができるってー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_010",
        "Text": "ねえ弾いてっ、なつき先生、ピアノォォーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_012",
        "Text": "綺麗な曲、あたしも弾いてみたいぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_015",
        "Text": "アトリん、さっき外に出てったよ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b203_016",
        "Text": "すぅ……すぅ……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b204_001",
        "Text": "特別授業だって",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b204_003",
        "Text": "みよちゃんカレシ持ってるのか？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b204_005",
        "Text": "うんとね、あたしも好きーーって答えるぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b204_006",
        "Text": "どうちがうんだ！？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b204_009",
        "Text": "……あたし、大人の階段登っちゃったんだ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b204_010",
        "Text": "ハンバーグはＬＯＶＥじゃなくってＬＩＫＥなんだよ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b206_001",
        "Text": "泳いでいい？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b206_002",
        "Text": "みなもちゃんもあれできる？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b206_003",
        "Text": "みなもちゃんやっぱすげー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b206_004",
        "Text": "アトリん、これ貸してあげる",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b207_002",
        "Text": "ロケットだよー！\\\\n今度こそ打ち上げやるって！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b207_004",
        "Text": "ロケットの事、教えてくれー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b207_005",
        "Text": "バイバイ、なつき先生、アトリん",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_004",
        "Text": "なつき先生、転校してっちゃう……？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_006",
        "Text": "ハナちゃんせんせー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_007",
        "Text": "何してたの？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_008",
        "Text": "それ知ってる、なつき先生の学校だ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_012",
        "Text": "ドンマイ、ハナちゃん",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_014",
        "Text": "外にね、知らない人がいる",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_015",
        "Text": "うん、見た事ないぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_017",
        "Text": "お仕事？あたしもあたしもっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_019",
        "Text": "言う事聞く",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_020",
        "Text": "あげなさいって！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_024",
        "Text": "うー……わかんないや",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_027",
        "Text": "うん、するー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_028",
        "Text": "？誰だろうあれ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_029",
        "Text": "ねぇ、あの人誰？ハナちゃんの知り合い？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b302_032",
        "Text": "うん……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_001",
        "Text": "ため息ダメだー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_002",
        "Text": "ため息つくと、婚期が遠のくって本に書いてあった",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_004",
        "Text": "ダメなんじゃないかなぁ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_006",
        "Text": "婚期が遠のく顔してる",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_007",
        "Text": "ふぅん",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_009",
        "Text": "ほら、この間の……学校の前にいた",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_010",
        "Text": "危ないよっ、すぐに教えないと！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b401_011",
        "Text": "ぅぅ……明日、教えてあげないと",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_001",
        "Text": "あっ、ハナちゃん先生！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_003",
        "Text": "うんとねー、アトリんトコ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_004",
        "Text": "どうしてー？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_005",
        "Text": "そーなんだよ、キケンなんだ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_007",
        "Text": "どうしたのハナちゃん……どこか痛い？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_008",
        "Text": "大変だっ、保健室行こう！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_009",
        "Text": "じゃああたしも行くぞ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_011",
        "Text": "ハナちゃん……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_012",
        "Text": "ッ……！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_013",
        "Text": "アトリんに危ないって知らせてくる！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_016",
        "Text": "ハァ、ハァ、大変なんだよアトリん！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_018",
        "Text": "うんそう！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_021",
        "Text": "うんそう！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_023",
        "Text": "ダメだって、危ないってばーー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_024",
        "Text": "ええっ、そうなのか！！？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_028",
        "Text": "あっ、なつき先生！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_030",
        "Text": "そいつ、前からアトリんを狙ってて……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_033",
        "Text": "なつき先生はどうするんだっ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b402_034",
        "Text": "うーー…………わかった！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_002",
        "Text": "アトリんだー",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_003",
        "Text": "だからー、アトリんの誕生日だよ",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_005",
        "Text": "なつき先生のピアノー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_006",
        "Text": "ハナちゃん先生はねぇ……",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_008",
        "Text": "って言ってた",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_009",
        "Text": "やろうぜー！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_010",
        "Text": "あたし、ぶわーーって噴火するやつやりたい！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_011",
        "Text": "こうやるんだぞ、なつき先生",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_012",
        "Text": "どうだ！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_014",
        "Text": "ラジャーであります！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_015",
        "Text": "取ってきたよリュウちゃん！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_017",
        "Text": "まだこれが残ってるよ、リュウちゃん！",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_018",
        "Text": "誰が火つける？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b406_019",
        "Text": "いーーち",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b501_002",
        "Text": "ねえ、なつき先生！\\\\nロケットどこから飛ぶの？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b501_003",
        "Text": "それってエデンのある方？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b501_005",
        "Text": "打ち上げ成功したの……？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b501_006",
        "Text": "なつき先生……行っちゃうの？",
        "Name": "凜々花"
    },
    {
        "File": "RIR_b501_007",
        "Text": "うんと勉強して、先生を追いかけるね！",
        "Name": "凜々花"
    }
]