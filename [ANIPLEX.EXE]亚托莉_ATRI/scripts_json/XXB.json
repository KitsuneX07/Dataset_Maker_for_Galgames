[
    {
        "File": "XXB_b111_001",
        "Text": "敵が攻めてきたってほんと？",
        "Name": "みよ"
    },
    {
        "File": "XXB_b111_002",
        "Text": "こわいよぉ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b111_003",
        "Text": "気をつけてね……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b111_004",
        "Text": "ヒットマンが……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b112_002",
        "Text": "さようなら、みなも先生",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_001",
        "Text": "はぁ、はぁ、セーフ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_002",
        "Text": "赤とか青のコードもある……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_003",
        "Text": "むずかしそう……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_004",
        "Text": "待ってよぉっ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_005",
        "Text": "私のもっ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_006",
        "Text": "……でっかければいいってものじゃないと思う",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_007",
        "Text": "でも自転車のタイヤ、どうやって取るの",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_008",
        "Text": "悪口言ってごめんなさい……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_009",
        "Text": "かして、やってあげる",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_011",
        "Text": "……これだけ？",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_012",
        "Text": "教室にある扇風機……動く？",
        "Name": "みよ"
    },
    {
        "File": "XXB_b113_013",
        "Text": "……うん",
        "Name": "みよ"
    },
    {
        "File": "XXB_b121_002",
        "Text": "みなもちゃん、普通にアウトだと思う",
        "Name": "みよ"
    },
    {
        "File": "XXB_b121_003",
        "Text": "みなもちゃん、カニちょうだいっ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b121_004",
        "Text": "あ……熱い……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b122_001",
        "Text": "ほんとだね",
        "Name": "みよ"
    },
    {
        "File": "XXB_b122_003",
        "Text": "みなもちゃんとお味噌汁作った",
        "Name": "みよ"
    },
    {
        "File": "XXB_b122_004",
        "Text": "死んじゃうの……？",
        "Name": "みよ"
    },
    {
        "File": "XXB_b122_005",
        "Text": "うん……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_001",
        "Text": "とおるクンっ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_002",
        "Text": "ごめんなさい……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_003",
        "Text": "扇風機動く……？",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_004",
        "Text": "なつき先生",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_005",
        "Text": "わぁ。。。なにこれ。。。。！！",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_006",
        "Text": "寝ないで遊んでいられるね",
        "Name": "みよ"
    },
    {
        "File": "XXB_b123_007",
        "Text": "はぅ。。、扇風機すずしいー",
        "Name": "みよ"
    },
    {
        "File": "XXB_b203_001",
        "Text": "お歌がうたえるね",
        "Name": "みよ"
    },
    {
        "File": "XXB_b203_002",
        "Text": "音楽の時間、どんな歌うたうんだろう",
        "Name": "みよ"
    },
    {
        "File": "XXB_b203_003",
        "Text": "あれがいい、あのカエルのやつ",
        "Name": "みよ"
    },
    {
        "File": "XXB_b203_004",
        "Text": "なぁに、この曲……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b204_004",
        "Text": "あのね、アトリちゃん。\\\\nアトリちゃんの告白は伝わらなかったでしょ？",
        "Name": "みよ"
    },
    {
        "File": "XXB_b302_001",
        "Text": "そんなのやだぁ……",
        "Name": "みよ"
    },
    {
        "File": "XXB_b406_001",
        "Text": "おめでとうアトリちゃん",
        "Name": "みよ"
    },
    {
        "File": "XXB_b406_002",
        "Text": "お友達の誕生日はみんなでお祝いするの",
        "Name": "みよ"
    }
]