[
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040356.ogg",
        "Text": "おぅっ",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040566.ogg",
        "Text": "お、相楽の彼氏だっけ？  みんなに紹介してんの？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040568.ogg",
        "Text": "そんなに血相変えて否定しなくてもいいじゃん",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040571.ogg",
        "Text": "何？  相楽のストーカー？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040573.ogg",
        "Text": "じゃ、俺が一度シメてやろうか？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040574.ogg",
        "Text": "でも、ひ弱そうだから、相楽ひとりで十分か",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040576.ogg",
        "Text": "なんてな。ほんと、困ってたら、言ってこいよ",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040678.ogg",
        "Text": "この前のストーカーくんじゃん",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040679.ogg",
        "Text": "また、相楽を待ってんの？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040681.ogg",
        "Text": "君、変質者には見えないんだけど",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040683.ogg",
        "Text": "ありゃ、そうなの",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040684.ogg",
        "Text": "でも、相楽の奴が好きで待ってるんだろ？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040686.ogg",
        "Text": "え？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040688.ogg",
        "Text": "ふぅん。それで付きまとってたのか",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040689.ogg",
        "Text": "あいつのことが好きってわけでもなかったのか…",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040690.ogg",
        "Text": "じゃあ、頼みたいことがあるんだけどさ、ちょっといいかな",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040692.ogg",
        "Text": "あいつの女友達に頼むのも、気が引けてさ…",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040693.ogg",
        "Text": "こういうのは男のほうがいいって思って",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040694.ogg",
        "Text": "相楽が俺のこと、慕ってくれてるのはわかってる",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040695.ogg",
        "Text": "でも、俺、こうして彼女がいるんだ",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040696.ogg",
        "Text": "最近は相楽も積極的になってきてるし…時間の問題かと思って",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040697.ogg",
        "Text": "今のうちにそれとなく伝えてやってもらえないかな",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040698.ogg",
        "Text": "俺も面と向かって言われたら、どう答えていいかわからないしさ…",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040700.ogg",
        "Text": "それは、性分なんだ、勘弁",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040701.ogg",
        "Text": "だからさ…な",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040703.ogg",
        "Text": "そ",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040704.ogg",
        "Text": "でも、気を回してあげてくれよな。あんまり傷つけないように",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040707.ogg",
        "Text": "ありゃ…困ったな",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040709.ogg",
        "Text": "でも、きっと…そのほうがいいよな？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040712.ogg",
        "Text": "だな",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040713.ogg",
        "Text": "じゃ、君…ええと、名前はなんて言ったっけか",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040715.ogg",
        "Text": "志麻くん、本当にすまないけど、よろしく頼むよ",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040785.ogg",
        "Text": "ダブルデート、いくかーーー？",
        "Name": "五十嵐"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0040\\Z004040786.ogg",
        "Text": "じょーだーーーん、じゃあなーーーーっ",
        "Name": "五十嵐"
    }
]