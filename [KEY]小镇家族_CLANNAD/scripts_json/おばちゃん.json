[
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0016\\Z001616870.ogg",
        "Text": "おやまあ、ちょっと早すぎるねえ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0016\\Z001616871.ogg",
        "Text": "食べ盛りだもんねぇ。好きなの買ってきな",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717153.ogg",
        "Text": "おやまあ、三日連続かい？",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717154.ogg",
        "Text": "おやおや、そうかい。そりゃまたすごいねえ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717155.ogg",
        "Text": "たくさん食べて、午後からたくさん勉強しなよ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717340.ogg",
        "Text": "おやまあ、今日は彼女とサボリかい？",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717341.ogg",
        "Text": "そうかい、そりゃすごいねえ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717348.ogg",
        "Text": "おやまあ…",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717349.ogg",
        "Text": "そうかい。あんたがあの、一ノ瀬さんのことみちゃんかね",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717350.ogg",
        "Text": "この町で長く暮らしてれば、知ってるって人もいるだろうね",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717351.ogg",
        "Text": "なにしろ、ご両親が立派な学者さんだからねえ…",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717352.ogg",
        "Text": "そうだともさ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717353.ogg",
        "Text": "元気そうで、何よりだねぇ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717354.ogg",
        "Text": "彼氏のおごりだろ？  好きなのいくつでも買ってもらいな",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818415.ogg",
        "Text": "はいことみちゃんこんにちは。元気かい？",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818417.ogg",
        "Text": "今日もちょっと早すぎるみたいだねえ。サボりかい？",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818419.ogg",
        "Text": "正直だねえ。そういう子はおばちゃん大好きさね",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818420.ogg",
        "Text": "オマケしてやるよ、ほらっ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818422.ogg",
        "Text": "サボりはよくないねえ、ちゃんと勉強しなきゃおばちゃん怒るよ？",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818423.ogg",
        "Text": "ほら、兄ちゃんにもオマケつけてやるから、午後はちゃんとやりなよ",
        "Name": "おばちゃん"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0030\\Z003030991.ogg",
        "Text": "お疲れさまです",
        "Name": "おばちゃん"
    }
]