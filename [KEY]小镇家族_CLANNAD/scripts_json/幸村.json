[
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0001\\Z000101297.ogg",
        "Text": "…来ないのかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0001\\Z000101322.ogg",
        "Text": "…来ないのかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0001\\Z000101337.ogg",
        "Text": "…来ないのかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606525.ogg",
        "Text": "いるがの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606528.ogg",
        "Text": "うむ…おはよう",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606533.ogg",
        "Text": "ふむ、まぁ…慣れとる",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606536.ogg",
        "Text": "ふむ、まぁ…こういうのも悪くない",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606537.ogg",
        "Text": "では、席についてもらえるかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606538.ogg",
        "Text": "む…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606539.ogg",
        "Text": "ひとり、見慣れない子が居ると思っての",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606542.ogg",
        "Text": "…可愛いの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606545.ogg",
        "Text": "それではまず…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606546.ogg",
        "Text": "一時間目は…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606547.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606548.ogg",
        "Text": "…現国にしようかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606553.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606575.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606580.ogg",
        "Text": "それでは、授業を始めるとしよう…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606581.ogg",
        "Text": "それでは教科書の…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606582.ogg",
        "Text": "…何ページにするかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606583.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606584.ogg",
        "Text": "では教科書…９ページ",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606585.ogg",
        "Text": "朗読を…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606587.ogg",
        "Text": "ふむ、伊吹くん",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606590.ogg",
        "Text": "ふむ…そうか。仕方がないの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606595.ogg",
        "Text": "ふむ…委員長の言うとおり",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606596.ogg",
        "Text": "苦手でも…やってみる",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606597.ogg",
        "Text": "すれば、なにか新しい発見が…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606598.ogg",
        "Text": "…あるやもしれん",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606603.ogg",
        "Text": "ふむ…ずいそう、だの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606605.ogg",
        "Text": "ふむ…そこまで",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606609.ogg",
        "Text": "ふむ…上出来",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606611.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606612.ogg",
        "Text": "そろそろ、終わるかの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606617.ogg",
        "Text": "あの子は…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606618.ogg",
        "Text": "伊吹先生の妹さんかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606619.ogg",
        "Text": "ふむ…元気そうでなにより…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606620.ogg",
        "Text": "ふむ…そうだの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606621.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606622.ogg",
        "Text": "長いこと、苦しんでいたからの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0006\\Z000606623.ogg",
        "Text": "本当に、よかった…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707324.ogg",
        "Text": "こんな行事に出てくるなんて、珍しい…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707325.ogg",
        "Text": "雨が降らずによかったの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707326.ogg",
        "Text": "今日はどうした…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707327.ogg",
        "Text": "ふむ…もちろん覚えとる…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707328.ogg",
        "Text": "胸毛がたくましかった…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707329.ogg",
        "Text": "冗談…わかっとる",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707330.ogg",
        "Text": "で、公子さんがどうした",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707331.ogg",
        "Text": "ふむ、めでたい",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707332.ogg",
        "Text": "おめでとう…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707333.ogg",
        "Text": "ふむ…それは早とちりだったの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707334.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707335.ogg",
        "Text": "ふむ…なるほど",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707336.ogg",
        "Text": "ない頭で、よく考えたの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707337.ogg",
        "Text": "ふむ…しかし…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707338.ogg",
        "Text": "公子さんが教えた生徒の多くは、卒業しておる…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707339.ogg",
        "Text": "なにか…あるのかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707340.ogg",
        "Text": "今日はどうした…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707341.ogg",
        "Text": "ふむ…伊吹先生か…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707342.ogg",
        "Text": "もちろん覚えとる…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707343.ogg",
        "Text": "胸毛がたくましかった…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707344.ogg",
        "Text": "女性…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707345.ogg",
        "Text": "ふぅむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707346.ogg",
        "Text": "名前だけ言われてものぅ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707347.ogg",
        "Text": "美人か…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707348.ogg",
        "Text": "ふむ、いたの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707349.ogg",
        "Text": "わしが美人だと思った女性教師はかつてひとりしか、在校しとらんからの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707350.ogg",
        "Text": "ふぉっふぉ…在校しとる女性教師には内緒だぞ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707351.ogg",
        "Text": "で、その公子さんがどうした",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707352.ogg",
        "Text": "ふむ、めでたい",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707353.ogg",
        "Text": "おめでとう…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707354.ogg",
        "Text": "ふむ…それは早とちりだったの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707355.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707356.ogg",
        "Text": "ふむ…なるほど",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707357.ogg",
        "Text": "ない頭で、よく考えたの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707358.ogg",
        "Text": "ふむ…しかし…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707359.ogg",
        "Text": "公子さんが教えた生徒の多くは、卒業しておる…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707360.ogg",
        "Text": "なにか…あるのかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707361.ogg",
        "Text": "ふむ…よぅわからんが…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707362.ogg",
        "Text": "なら、教頭や校長に掛け合ってみるかの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707363.ogg",
        "Text": "ふむ、ぺいぺいだの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707364.ogg",
        "Text": "しかし、守らなければならん面子も世間体もある",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707365.ogg",
        "Text": "果たしてどうかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707366.ogg",
        "Text": "ふむ…そうだの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707367.ogg",
        "Text": "ふむ…まさしく",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707368.ogg",
        "Text": "あの…公子さんだからの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707888.ogg",
        "Text": "見てわからんか…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707889.ogg",
        "Text": "不思議なことを訊くの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707890.ogg",
        "Text": "その若さで、ボケたかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707891.ogg",
        "Text": "ふむ、なら、思い出してみい",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707892.ogg",
        "Text": "ふむ、そうだの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707893.ogg",
        "Text": "ふむ…なにかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707894.ogg",
        "Text": "また不思議なことを言う…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707895.ogg",
        "Text": "そういう間柄だから…であろうに",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707896.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707897.ogg",
        "Text": "それは…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707898.ogg",
        "Text": "妹さんではないかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707899.ogg",
        "Text": "ふむ…悪くない",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707900.ogg",
        "Text": "…それではの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707901.ogg",
        "Text": "名前か…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707902.ogg",
        "Text": "ふむ…確か…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707903.ogg",
        "Text": "…\\wait{600}風子、と",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0007\\Z000707918.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0009\\Z000909987.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0009\\Z000909988.ogg",
        "Text": "いいのぉ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0009\\Z000909989.ogg",
        "Text": "若い頃を思い出す…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0011\\Z001111492.ogg",
        "Text": "ほわちゃ！",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0020\\Z002020447.ogg",
        "Text": "…来ないのかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424148.ogg",
        "Text": "ん…なにかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424150.ogg",
        "Text": "ああ、うむ",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424152.ogg",
        "Text": "ほぅ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424153.ogg",
        "Text": "ほぅ、ほぅ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424164.ogg",
        "Text": "うん…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424166.ogg",
        "Text": "いや…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424169.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424170.ogg",
        "Text": "二年生のね、仁科さんって子とね…話をしてくれないですかな",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424173.ogg",
        "Text": "確か…Ｂだったような",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424174.ogg",
        "Text": "あるいはＣだったような…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424176.ogg",
        "Text": "たぶんＣだ。ふむ、間違いない",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424507.ogg",
        "Text": "いいの…ですかな",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424510.ogg",
        "Text": "そう…ですか。今時…演劇なんて流行らないのですかな",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0024\\Z002424513.ogg",
        "Text": "ふむ…また…機会もあるでしょう",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0025\\Z002525045.ogg",
        "Text": "ふむ…なんか用かの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0025\\Z002525046.ogg",
        "Text": "ふむ…昔はの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0025\\Z002525061.ogg",
        "Text": "そうか…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828168.ogg",
        "Text": "うむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828169.ogg",
        "Text": "そうだの…いろいろといる",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828172.ogg",
        "Text": "大切なのはみっつ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828173.ogg",
        "Text": "美術、音響と、照明…だの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828174.ogg",
        "Text": "美術は、衣装や、舞台道具を用意する",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828175.ogg",
        "Text": "音響は…あらかじめテープに編集しておいた背景音楽や効果音を流す",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828176.ogg",
        "Text": "照明は…数種類のライトを使い分け、舞台を効果的に照らし出す",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828177.ogg",
        "Text": "もし、三人しかいないというのであれば…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828178.ogg",
        "Text": "美術は先に用意し…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828179.ogg",
        "Text": "残りを役者と音響と照明に振り分けるのがよかろう…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828182.ogg",
        "Text": "そうそう…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828183.ogg",
        "Text": "…今週は演劇部、合唱部…どちらの番だったかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828186.ogg",
        "Text": "うむ…では、放課後",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828194.ogg",
        "Text": "どれだけ大変かは劇による…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828195.ogg",
        "Text": "そんな展開の早い劇をやるつもりかな…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828197.ogg",
        "Text": "ふむ…なら、簡単だろうて",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828199.ogg",
        "Text": "効果音は、その鍵盤で鳴らすがよい",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828226.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828227.ogg",
        "Text": "音楽はたくさん聴くかの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828228.ogg",
        "Text": "ふむ…だとしたら…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828229.ogg",
        "Text": "音楽室にある教材のレコードから、場面に合った音楽を探すのがよかろう…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828231.ogg",
        "Text": "ふむ…それ以外はなかろうな",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828394.ogg",
        "Text": "ほぅ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828395.ogg",
        "Text": "より、親しみやすくなっとる…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0028\\Z002828397.ogg",
        "Text": "ふむ…なかなか…できることではない",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232378.ogg",
        "Text": "これより、古河渚とその親友一同、その卒業式を行います",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232379.ogg",
        "Text": "校歌斉唱",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232380.ogg",
        "Text": "…古河渚",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232437.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232438.ogg",
        "Text": "卒業…\\pおめでとう",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232440.ogg",
        "Text": "以上…\\p閉式します",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232445.ogg",
        "Text": "以上…\\p閉式します",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232449.ogg",
        "Text": "以上…\\p閉式します",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0032\\Z003232453.ogg",
        "Text": "以上…\\p閉式します",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636791.ogg",
        "Text": "最後ぐらい、出んかい…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636796.ogg",
        "Text": "ほんとに、おまえらは…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636797.ogg",
        "Text": "情けないやつらだの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636798.ogg",
        "Text": "これからは、社会人というのにの…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636799.ogg",
        "Text": "ふむ…まぁ、それもそうか…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636800.ogg",
        "Text": "ま、ホームルームぐらい出たほうがいい…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636801.ogg",
        "Text": "乾が悲しむでの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636803.ogg",
        "Text": "おまえらの担任だの",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636805.ogg",
        "Text": "ふむ…まぁ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636806.ogg",
        "Text": "それだけだ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636807.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636808.ogg",
        "Text": "自分の教え子は…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636809.ogg",
        "Text": "例外なく、自分の子供だと思っておる…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636810.ogg",
        "Text": "ただ…この学校は…ちと優秀すぎる生徒が多すぎての…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636811.ogg",
        "Text": "長い間、わしの出番はなかった…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636812.ogg",
        "Text": "が…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636813.ogg",
        "Text": "最後に、おまえらの面倒を見られてよかった…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636814.ogg",
        "Text": "ふむ…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636815.ogg",
        "Text": "頑張りなさい…",
        "Name": "幸村"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0036\\Z003636820.ogg",
        "Text": "無理を言うな…",
        "Name": "幸村"
    }
]