[
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717843.ogg",
        "Text": "あの…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717849.ogg",
        "Text": "あっ、すみません",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717850.ogg",
        "Text": "もしかしたら、音、うるさかったですか？",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717851.ogg",
        "Text": "すぐに仕舞いますから…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717855.ogg",
        "Text": "あの…ありがとうございます",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717858.ogg",
        "Text": "いえ…そうじゃないんです",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717859.ogg",
        "Text": "このヴァイオリン、音楽室に置きっぱなしにされていたそうなんです",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717860.ogg",
        "Text": "卒業した音楽部員が置いていったらしいんですが…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717861.ogg",
        "Text": "それで、音楽部の知り合いから、価値のある物なのか調べてほしいって言われたんです",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717863.ogg",
        "Text": "それほどでもないですけど、少しぐらいは",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717866.ogg",
        "Text": "ええと…ちゃんとした入門用のヴァイオリンなんですけど、あちこち痛んでるから…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717867.ogg",
        "Text": "できれば修理して、ちゃんと使ってあげてほしいと思うんですが",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717869.ogg",
        "Text": "あの、よかったら、ちょっとだけ弾いてみませんか？",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717875.ogg",
        "Text": "それなら、ぜひどうぞ",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717881.ogg",
        "Text": "……あの",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717890.ogg",
        "Text": "力の入れすぎです、もっとリラックスしてっ…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717913.ogg",
        "Text": "すみません。ちゃんと調弦してないから…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717914.ogg",
        "Text": "本当は弦も張り替えてあげればいいんですけど…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717924.ogg",
        "Text": "やっぱり。姿勢とか、きちんとしてましたから",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717925.ogg",
        "Text": "レッスン、好きでした？",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717926.ogg",
        "Text": "いいなあ。わたしは大嫌いだったかも",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717928.ogg",
        "Text": "あの…よかったら、何日か使ってみませんか？",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717929.ogg",
        "Text": "音楽部の方には、わたしから説明しておきますから",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717931.ogg",
        "Text": "この子、ずっと放っておかれてましたけど…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717932.ogg",
        "Text": "きっと、誰かが弾いてくれるのを待っていたんだと思います",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717933.ogg",
        "Text": "少しの間でも、もう一度音を奏でたいって、思ってます。きっと",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0017\\Z001717934.ogg",
        "Text": "どうぞ…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818437.ogg",
        "Text": "あっ…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818439.ogg",
        "Text": "はい。どうでしたか？",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818442.ogg",
        "Text": "あの…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818443.ogg",
        "Text": "もしよかったら、もう少し長く借りられるように、わたしから頼んでみますけど…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818451.ogg",
        "Text": "わたしも、そう言うつもりですから",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818452.ogg",
        "Text": "いえ…",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818457.ogg",
        "Text": "はい",
        "Name": "合唱部員"
    },
    {
        "File": "D:\\ghast\\downloads\\CLANNAD\\KOE\\0018\\Z001818458.ogg",
        "Text": "わたしは、仁科りえと言います",
        "Name": "合唱部員"
    }
]